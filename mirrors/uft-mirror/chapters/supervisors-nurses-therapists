<!-- Obits -->


if ( $('body').hasClass('.node-type-obituaries') )
{

var editobit = $('.node-type-obituaries .imagecache-portrait_small').attr('src');
var editPos = editobit.search("/photo/");
var editObitPhoto = editobit.slice(editPos + 7);
var editNewObitPhotoURL = "http://www.uft.org/files/imagecache/portrait_small/photo/" + editObitPhoto;

$('.node-type-obituaries .imagecache-portrait_small').attr('src', editNewObitPhotoURL);

}






<!-- Instagram Galleries -->

$('body.page-galleries-instagram-uftgopink-2015 .instaheader').css({'background-color': '#f447a5', 'float': 'left', 'width': '282px', 'color': '#fff', 'padding': '1.25em', 'text-align': 'left', 'margin-top': '20px', 'border-radius': '1.25em', 'position': 'relative', 'z-index': '100' });

