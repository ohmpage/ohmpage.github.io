$(teachContract).attr('selected','selected');


<!-- image resizing -->

if ( $('body').hasClass('.node-type-clupdates') || $('body').hasClass('.node-type-article') )
{

$('#content-area .nrembrowser-styles').attr('style', '');


$('.nrembrowser-styles img').each( function(){

var imgWidth = $(this).width();

$(this).parents('.styles-wrapper').addClass("stretch-wrapper _" + imgWidth);

$(this).addClass("stretch-img _" + imgWidth);


});


}



<!-- Remove inline styles + add class to embedded photos on article and pages -->


if( $('body').hasClass('.node-type-article') || $('body').hasClass('.node-type-page')  ) {


var stripCSS = new Array();


$('#content-area .content span.nrembrowser-styles').each( function(){


var spanWidth = $(this).width();
	
        $(this).addClass('width-' + spanWidth);

	$(this).attr('style', '');


});

}



<!-- Remove PDF icon from images that link to pdfs -->

var gotPDF = $('#content-area a[href$=".pdf"]  img');

if(gotPDF) {
   $(gotPDF).parents('a').addClass('remove-pdf-icon');
   }


 




<!-- NYT article: to teach is to touch lives forever -->

$('.content-section').after(" ").css({'display':'block', 'clear': 'both'});

$('.content-section').css('border-bottom', '1px solid #541a33');

$('.clear-section').css('clear', 'both');

$('.content-section:nth-child(n + 2)').css('padding-top', '2em');

$('.content-section:nth-last-of-type(1)').css({'border-bottom-width': '0','padding-bottom': '0'});

$('.content-section-footer').css('font-weight', 'bold');

$('.align-right').css({'float':'right', 'text-align':'right'});

<!-- quick fixes to be moved to code -->

